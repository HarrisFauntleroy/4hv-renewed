{
  "name": "strongly-typed",
  "version": "1.0.1",
  "private": true,
  "homepage": "https://strongly-typed.vercel.app/",
  "repository": {
    "type": "git",
    "url": "https://github.com/HarrisFauntleroy/strongly-typed"
  },
  "license": "MIT",
  "author": {
    "name": "Harris Fauntleroy",
    "url": "https://github.com/HarrisFauntleroy"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "prisma generate",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "build-storybook": "storybook build",
    "clean": "rimraf .turbo node_modules dist tsconfig.tsbuildinfo",
    "dev": "next dev -p 3000",
    "lint": "yarn esw . --ext .js,.jsx,.ts,.tsx --color",
    "lint:fix": "yarn esw . --ext .js,.jsx,.ts,.tsx --fix --color",
    "lint:watch": "yarn esw . --ext .js,.jsx,.ts,.tsx --watch --fix --color --changed --clear",
    "format": "prettier --write .",
    "reset-db": "docker-compose down --volumes --remove-orphans",
    "start": "next start",
    "storybook": "storybook dev -p 6007",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static --port 6007 --silent",
    "swagger": "next-swagger-doc-cli next-swagger-doc.json",
    "test": "jest --passWithNoTests",
    "test:ci": "yarn test --ci --coverage --maxWorkers=2",
    "test:watch": "yarn test --watchAll",
    "type-check": "tsc --noEmit",
    "watch": "yarn type-check --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn format"
    ]
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "strict",
    "jsxSingleQuote": false,
    "printWidth": 80,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false,
    "vueIndentScriptAndStyle": false
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "settings": {
      "import/resolver": {
        "typescript": true,
        "node": true
      },
      "react": {
        "version": "detect"
      }
    },
    "plugins": [
      "@typescript-eslint",
      "simple-import-sort",
      "import",
      "sonarjs",
      "jsx-a11y",
      "prettier"
    ],
    "extends": [
      "next",
      "plugin:@typescript-eslint/recommended",
      "eslint:recommended",
      "plugin:import/typescript",
      "plugin:import/recommended",
      "plugin:sonarjs/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:storybook/recommended",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "import/extensions": "off",
      "simple-import-sort/imports": [
        "off",
        {
          "groups": [
            [
              "^react$"
            ],
            [
              "^(ui|common|database|database/generated/prisma-client)$"
            ],
            [
              "^[./]"
            ]
          ]
        }
      ],
      "@next/next/no-html-link-for-pages": [
        "error",
        "pages"
      ]
    },
    "ignorePatterns": [
      "dist"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": ".test.*",
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  },
  "dependencies": {
    "@chakra-ui/icons": "2.0.19",
    "@chakra-ui/react": "^2.5.5",
    "@chakra-ui/theme-tools": "2.0.16",
    "@choc-ui/chakra-autocomplete": "^5.1.4",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@next-auth/prisma-adapter": "1.0.6",
    "@playwright/test": "^1.27.1",
    "@prisma/client": "4.4.0",
    "@tanstack/match-sorter-utils": "^8.5.14",
    "@tanstack/react-table": "^8.5.30",
    "@trpc/client": "9.27.2",
    "@trpc/next": "9.27.2",
    "@trpc/react": "9.27.2",
    "@trpc/server": "9.27.2",
    "@uiw/react-markdown-preview": "^4.1.5",
    "@uiw/react-md-editor": "3.18.1",
    "babel-plugin-superjson-next": "0.4.5",
    "chakra-ui-steps": "^1.8.0",
    "clsx": "1.2.1",
    "framer-motion": "7.4.0",
    "katex": "0.16.7",
    "mermaid": "9.1.7",
    "next": "13.3.4",
    "next-auth": "^4.18.7",
    "next-remove-imports": "1.0.7",
    "next-seo": "^5.14.1",
    "nodemailer": "6.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-focus-lock": "2.9.4",
    "react-hook-form": "7.36.1",
    "react-icons": "4.4.0",
    "react-intl": "6.1.2",
    "react-markdown": "8.0.7",
    "react-query": "3.39.2",
    "react-syntax-highlighter": "15.5.0",
    "redis": "4.3.1",
    "rehype-katex": "6.0.2",
    "rehype-stringify": "9.0.3",
    "remark-breaks": "3.0.2",
    "remark-frontmatter": "4.0.1",
    "remark-gfm": "3.0.1",
    "remark-math": "5.1.1",
    "remark-parse": "10.0.1",
    "remarkable": "2.0.1",
    "remove": "^0.1.5",
    "superjson": "1.12.2",
    "unified": "10.1.2",
    "uuid": "9.0.0",
    "ws": "8.13.0",
    "zod": "3.21.4",
    "zod-prisma-types": "^2.5.6"
  },
  "devDependencies": {
    "@jest/types": "29.5.0",
    "@storybook/addon-essentials": "^7.0.4",
    "@storybook/addon-interactions": "^7.0.4",
    "@storybook/addon-links": "^7.0.4",
    "@storybook/blocks": "^7.0.4",
    "@storybook/nextjs": "^7.0.4",
    "@storybook/react": "^7.0.4",
    "@storybook/testing-library": "^0.1.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.11.6",
    "@types/nodemailer": "6.4.7",
    "@types/papaparse": "^5.3.7",
    "@types/react": "18.2.7",
    "@types/react-dom": "^18.0.11",
    "@types/swagger-ui-react": "4.18.0",
    "@types/uuid": "9.0.1",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.58.0",
    "cross-env": "7.0.3",
    "dotenv": "16.1.0",
    "eslint": "^8.41.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "0.19.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-watch": "^8.0.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "lint-staged": "13.2.2",
    "next-sitemap": "3.1.55",
    "npm-run-all": "4.1.5",
    "playwright": "1.34.3",
    "prettier": "2.8.8",
    "prisma": "4.15.0",
    "storybook": "^7.0.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "prisma": {
    "seed": "yarn ts-node prisma/seed.ts"
  }
}
